{
  "name": "@bltzr-gg/token-sale",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port ${VITE_APP_PORT-3000}",
    "build": "tsc -p tsconfig.build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:visual-tests": "source .env && chromatic --project-token=$STORYBOOK_CHROMATIC_PROJECT_TOKEN",
    "generate:openapi": "pnpm --filter=@bltzr-gg/cloak run generate:openapi",
    "env:testnet-mock": "dotenvx run -f .env.testnet-mock",
    "env:mainnet-mock": "dotenvx run -f .env.mainnet-mock",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome --spec 'cypress/smoke.cy.ts'",
    "cy:smoke": "start-server-and-test dev ${VITE_APP_URL} \"pnpm cy:run\"",
    "test:smoke:mainnet": "pnpm env:mainnet-mock -- pnpm cy:smoke",
    "test:smoke:testnet": "pnpm env:testnet-mock -- pnpm cy:smoke",
    "test:unit": "vitest run",
    "test": "pnpm run test:unit && pnpm /^test:smoke:/"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@axis-finance/abis": "0.0.3",
    "@axis-finance/cloak": "0.0.13",
    "@axis-finance/deployments": "0.0.9",
    "@axis-finance/env": "0.0.13",
    "@axis-finance/sdk": "0.0.34",
    "@axis-finance/subgraph-client": "0.0.11",
    "@axis-finance/types": "0.0.21",
    "@bltzr-gg/eslint-config": "workspace:*",
    "@bltzr-gg/typescript-config": "workspace:*",
    "@bltzr-gg/ui": "workspace:*",
    "@hookform/resolvers": "^4.1.3",
    "@rainbow-me/rainbowkit": "^2.0.2",
    "@tanstack/react-query": "5.18.1",
    "@tanstack/react-query-devtools": "^5.18.1",
    "@tanstack/react-table": "^8.11.7",
    "date-fns": "^3.3.1",
    "graphql": "16.8.1",
    "jotai": "^2.7.0",
    "lucide-react": "^0.439.0",
    "react": "18.3.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "recharts": "^2.12.0",
    "viem": "^2.17.3",
    "vite-plugin-node-polyfills": "^0.17.0",
    "vite-tsconfig-paths": "^4.3.1",
    "wagmi": "^2.5.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.7.0",
    "@hookform/devtools": "^4.3.1",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/node": "^20.11.5",
    "@types/react": "18.2.48",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^18.2.18",
    "@types/react-scroll": "^1.8.10",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cypress": "^13.13.3",
    "eslint": "^8.55.0",
    "eslint-plugin-storybook": "^0.8.0",
    "msw": "^2.3.5",
    "postcss": "^8.4.33",
    "start-server-and-test": "^2.0.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-motion": "^1.1.0",
    "typescript": "5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.5.0"
  }
}
